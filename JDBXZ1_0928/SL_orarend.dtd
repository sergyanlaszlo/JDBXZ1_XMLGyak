        <!ATTLIST helyszin
                id CDATA #REQUIRED
                nev CDATA #REQUIRED>
        <!ELEMENT oktato (#PCDATA)>
        <!ATTLIST oktato
                id CDATA #REQUIRED
                nev CDATA #REQUIRED>
